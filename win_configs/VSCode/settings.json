{
  "editor.fontLigatures": true,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace, dotum",
  "comment": "fontFamily@#code",
  "editor.fontVariations": true,
  "editor.fontWeight": "400",
  "git.autofetch": true,
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "git.enableSmartCommit": true,
  "cmake.showOptionsMovedNotification": false,
  "cmake.generator": "Ninja",
  "cmake.configureOnOpen": true,
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "editor.fontSize": 11.5,
  "git.confirmSync": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "g",
        "g"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    }
  ],
  "comments": "vimNormalKeyBind@#code",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "vim.autoSwitchInputMethod.enable": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME\\.ideavimrc",
  "workbench.iconTheme": "catppuccin-frappe",
  "catppuccin.italicKeywords": false,
  "catppuccin.italicComments": false,
  "vim.smartRelativeLine": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace, dotum",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "terminal.integrated.cursorBlinking": true,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "powershell.powerShellAdditionalExePaths": {
    "pwsh": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
  },
  "powershell.powerShellDefaultVersion": "pwsh",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    },
    "MSYS2 UCRT": {
      "path": "cmd.exe",
      "args": [
        "/c",
        "C:\\msys64\\msys2_shell.cmd -defterm -here -no-start -ucrt64"
      ]
    }
  },
  "window.customTitleBarVisibility": "windowed",
  "window.commandCenter": false,
  "maui.configuration.androidSdkPreferredPath": "C:\\Program Files (x86)\\Android\\android-sdk",
  "maui.configuration.debugVerbosity": "detailed",
  "editor.minimap.autohide": true,
  "window.zoomLevel": 1.15,
  "redhat.telemetry.enabled": false,
  "vscode-ahk-plus.compilePath": "C:\\Program Files\\AutoHotkey\\Compiler\\Ahk2Exe.exe",
  "vscode-ahk-plus.documentPath": "C:\\Program Files\\AutoHotkey\\v2\\AutoHotkey.chm",
  "vscode-ahk-plus.executePath": "C:\\Program Files\\AutoHotkey\\v2\\AutoHotkey64.exe",
  "AHK++.v1.formatter": {
    "allowedNumberOfEmptyLines": 1,
    "preserveIndent": true
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "remote.autoForwardPortsSource": "hybrid",
  "rust-analyzer.check.command": "clippy",
  "C_Cpp.default.compilerPath": "C:/msys64/ucrt64/bin/gcc.exe",
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "terminal.integrated.splitCwd": "workspaceRoot",
  "comments": 
    "termial split@#code",
  "pythonIndent.useTabOnHangingIndent": true,
  "pythonIndent.trimLinesWithOnlyWhitespace": true,
  "pythonIndent.keepHangingBracketOnLine": true,
  "AHK++.v2.formatter": {
    "indentString": "  ",
    "maxPreserveNewlines": 2,
    "wrapLineLength": 120
  }
}