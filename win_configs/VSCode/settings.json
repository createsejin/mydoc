{
  "files.autoSave": "afterDelay",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'SauceCodePro Nerd Font', Consolas, 'Courier New', monospace, dotum",
  "editor.fontVariations": false,
  "editor.fontWeight": "374",
  "git.autofetch": true,
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "git.enableSmartCommit": true,
  "cmake.showOptionsMovedNotification": false,
  "cmake.generator": "Ninja",
  "cmake.configureOnOpen": true,
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "editor.fontSize": 11.5,
  "git.confirmSync": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "1033",
  "vim.autoSwitchInputMethod.obtainIMCmd": "C:\\tools\\im-select.exe",
  "vim.autoSwitchInputMethod.switchIMCmd": "C:\\tools\\im-select.exe {im}",
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "C:\\Users\\creat\\.ideavimrc",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "catppuccin.italicKeywords": false,
  "catppuccin.italicComments": false,
  "vim.smartRelativeLine": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font', Consolas, 'Courier New', monospace, dotum",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "terminal.integrated.cursorBlinking": true,
  "window.zoomLevel": 1,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "vim.useSystemClipboard": true,
  "powershell.powerShellAdditionalExePaths": {
    "pwsh": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
  },
  "powershell.powerShellDefaultVersion": "pwsh",
  "terminal.integrated.profiles.windows": {
    
    "PowerShell": {
      "source": "PowerShell",
      "args": ["-NoExit", "-Command", "[Console]::OutputEncoding = [System.Text.Encoding]::UTF8; [Console]::InputEncoding = [System.Text.Encoding]::UTF8"],
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
}